{
    "backend": {
            "file": 
                {"path": "/vault/vault_data"}
    },
    "listener": {
            "tcp": {"address": "0.0.0.0:8200", 
                    "tls_disable": 0, 
                    "tls_cert_file": "/vault/config/cert.pem", 
                    "tls_key_file": "/vault/config/cert.key"
                    }
            },
    "default_lease_ttl": "168h",
    "max_lease_ttl": "0h"
}
